{
  "name": "vite-plugin-solid-pages",
  "type": "module",
  "version": "0.3.4",
  "packageManager": "pnpm@10.12.1",
  "description": "vite-plugin-solid-pages",
  "license": "MIT",
  "homepage": "https://github.com/g-mero/vite-plugin-solid-pages",
  "repository": {
    "type": "git",
    "url": "https://github.com/g-mero/vite-plugin-solid-pages"
  },
  "bugs": {
    "url": "https://github.com/g-mero/vite-plugin-solid-pages/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "after-build": "node ./scripts/after-build.mjs",
    "build": "tsup && pnpm after-build",
    "clean": "rimraf dist node_modules pnpm-lock.yaml",
    "dev": "vite -c ./playground/vite.config.ts",
    "pub": "pnpm build && cd dist && pnpm publish",
    "test": "vitest --run",
    "lint": "npx ultracite lint",
    "lint:fix": "npx ultracite format",
    "type-check": "tsc --noEmit --skipLibCheck",
    "release": "commit-and-tag-version -i CHANGELOG.md --same-file"
  },
  "peerDependencies": {
    "@solidjs/router": "^0.15.2",
    "vite": "^6.0.3"
  },
  "dependencies": {
    "@swc/core": "^1.13.3",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "gray-matter": "^4.0.3",
    "radash": "^12.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@mdx-js/rollup": "^3.1.0",
    "@solidjs/router": "^0.15.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.3.0",
    "commit-and-tag-version": "^12.5.0",
    "husky": "^9.1.7",
    "rehype-pretty-code": "^0.14.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "solid-js": "^1.9.9",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "ultracite": "5.2.3",
    "unocss": "^66.0.0",
    "vite": "^6.0.3",
    "vite-plugin-solid": "^2.11.8",
    "vitest": "^3.2.4"
  }
}
