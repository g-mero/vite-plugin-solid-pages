{
  "name": "vite-plugin-solid-pages",
  "type": "module",
  "version": "0.3.1",
  "description": "vite-plugin-solid-pages",
  "license": "MIT",
  "homepage": "https://github.com/g-mero/vite-plugin-solid-pages",
  "repository": {
    "type": "git",
    "url": "https://github.com/g-mero/vite-plugin-solid-pages"
  },
  "bugs": {
    "url": "https://github.com/g-mero/vite-plugin-solid-pages/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "after-build": "node ./scripts/after-build.mjs",
    "build": "tsup && pnpm after-build",
    "dev": "tsup --watch",
    "pub": " pnpm build && cd dist && pnpm publish",
    "test": "vitest --run",
    "lint": "pnpm exec eslint .",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "@solidjs/router": "^0.15.2",
    "vite": "^6.0.3"
  },
  "dependencies": {
    "@swc/core": "^1.10.4",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "radash": "^12.1.0"
  },
  "devDependencies": {
    "@repo/ts-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.2",
    "@vitest/ui": "3.2.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unocss": "^66.0.0",
    "vite": "^6.0.3",
    "vitest": "^3.0.0"
  }
}
